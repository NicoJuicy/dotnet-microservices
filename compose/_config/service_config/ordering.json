{
  "service": {
    "name": "ordering",
    "id":"ordering-v1",
    "port": 8001,
    "address":"10.0.0.3",
    "checks": [
      {
        "id": "ordering-api",
        "name": "HTTP API on port 8001",
        "http": "http://10.0.0.3:8001/health",
        "interval": "10s"
      }
    ],
    "tags":[
      "ordering",
      "urlprefix-/ordering/ strip=/ordering"
    ],
    "connect": {
      "sidecar_service": {
        "port": 20000,
        "checks": [
          {
          "name": "Connect Envoy Sidecar",
          "tcp": "10.0.0.3:20000",
          "interval": "10s"
          },
          {
          "name": "Connect Sidecar Aliasing ordering",
          "alias_service": "ordering"
          }
        ],
        "tags":[
          "ordering"
        ]
      }
    }
  }
}
